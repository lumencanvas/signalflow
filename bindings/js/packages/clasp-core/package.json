    "lint": "eslint src"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "license": "MIT",
  "author": "LumenCanvas",
  "homepage": "https://clasp.to",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumencanvas/clasp.git"
  },
  "bugs": {
    "url": "https://github.com/lumencanvas/clasp/issues"
  },
  "keywords": [
    "clasp",
    "clasp-to",
    "osc",
    "midi",
    "dmx",
    "artnet",
    "mqtt",
    "websocket",
